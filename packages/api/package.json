    {
      "path": "dist/api.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.2",
    "size-limit": "^4.9.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "ajv": "^7.2.1",
    "axios": "^0.21.1",
    "axios-logger": "^2.5.0",
    "qs": "^6.9.6"
  }
}
