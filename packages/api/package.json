{
  "name": "@nectar/api",
  "version": "0.0.1-alpha.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch --verbose",
    "build": "tsdx build",
    "test": "tsdx test",
