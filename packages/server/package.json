{
  "name": "@nectar/server",
  "author": "Tim Hostetler",
  "version": "0.0.1-alpha.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "private": "true",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "NODE_ENV=production node dist/index.js",
    "dev": "node dist/index.js",
    "watch": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "module": "dist/server.esm.js",
  "size-limit": [
    {
      "path": "dist/server.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/server.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.2",
    "size-limit": "^4.9.2",
    "supertest": "^6.1.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@nectar/api": "^0.0.1-alpha.0",
    "@types/express": "^4.17.11",
    "@types/express-pino-logger": "^4.0.2",
    "@types/pino": "^6.3.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-pino-logger": "^6.0.0",
    "next": "^10.0.7",
    "pino": "^6.11.1",
    "pino-pretty": "^4.5.0"
  },
  "volta": {
    "node": "14.15.5"
  }
