{
  "name": "nectar",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:css": "tailwind build styles/index.tailwind.css -o styles/index.css",
    "watch:css": "nodemon -w styles/index.tailwind.css -w tailwind.config.js -e css,js -x yarn build:css",
    "debug": "NODE_OPTIONS='--inspect=9090' ts-node --project tsconfig.server.json server/index.ts",
    "dev": "NODE_ENV=development ts-node --project tsconfig.server.json server/index.ts",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "build": "NODE_ENV=production yarn run build:css && yarn run build:next && yarn run build:server",
    "start": "NODE_ENV=production node dist/index.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "@next/bundle-analyzer": "^10.0.0",
    "@tailwindcss/ui": "^0.7.0",
    "@types/cookie": "^0.4.0",
    "@types/cookie-session": "^2.0.41",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/express-rate-limit": "^5.1.0",
    "@types/morgan": "^1.9.1",
    "@types/qs": "^6.9.4",
    "@types/ramda": "^0.27.14",
    "@xstate/fsm": "^1.5.1",
    "@xstate/react": "^1.0.3",
    "autoprefixer": "10.0.2",
    "axios": "^0.21.0",
    "axios-debug-log": "^0.8.0",
    "clsx": "^1.1.1",
    "cookie": "^0.4.1",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^8.0.0",
    "morgan": "^1.10.0",
    "next": "^10.0.2-canary.2",
    "next-compose-plugins": "^2.2.0",
    "postcss": "^8.1.10",
    "postcss-import": "^13.0.0",
    "qs": "^6.9.4",
    "ramda": "^0.27.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-error-boundary": "^3.0.2",
    "recoil": "^0.1.2",
    "tailwindcss": "^2.0.1",
    "typeface-roboto": "^1.1.13",
    "use-dark-mode": "^2.3.1",
    "xstate": "^4.13.0"
  },
  "devDependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "@types/node": "^14.6.1",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.7.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react-hooks": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "2.2.0",
    "typescript": "^4.0.2"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11",
    "maintained node versions"
  ]
